OBJECT        = COLUMN                                                        
   NAME           = BUFFER_OVERFLOW                                           
   COLUMN_NUMBER  = 21                                                        
   BYTES          = 2                                                         
   DATA_TYPE      = MSB_UNSIGNED_INTEGER                                      
